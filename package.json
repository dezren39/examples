{
  "name": "examples",
  "private": true,
  "description": "A set of examples for Motion Canvas",
  "license": "MIT",
  "scripts": {
    "deferred-lighting": "npm run serve -w examples/deferred-lighting",
    "motion-canvas": "npm run serve -w examples/motion-canvas",
    "lint": "eslint \"examples/**/src/**/*.ts?(x)\"",
